{
  "name": "@netcracker/qip-ui",
  "version": "0.0.38-dev-4",
  "license": "Apache-2.0",
  "description": "Qubership Integration Platform - UI",
  "module": "./dist-lib/index.es.js",
  "types": "./dist-lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist-lib/index.es.js",
      "types": "./dist-lib/types/index.d.ts"
    },
    "./external": {
      "import": "./dist-lib/index.es.js",
      "types": "./dist-lib/types/index.d.ts"
    },
    "./bundled": {
      "import": "./dist-lib/index.bundled.es.js",
      "types": "./dist-lib/types/index.d.ts"
    },
    "./qip-ui.css": "./dist-lib/qip-ui.css",
    "./dist-lib/qip-ui.css": "./dist-lib/qip-ui.css",
    "./scripts/fetch-documentation": "./scripts/fetch-documentation.mjs",
    "./scripts/build-doc-index": "./scripts/build-doc-index.mjs"
  },
  "files": [
    "dist-lib",
    "scripts/fetch-documentation.mjs",
    "scripts/build-doc-index.mjs",
    ".documentation-config.json.example"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prebuild": "npm run fetch-docs",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:lib:external": "vite build --config vite.config.lib.ts",
    "build:lib:bundled": "vite build --config vite.config.lib.bundled.ts",
    "build:lib:all": "npm run build:lib:external && npm run build:lib:bundled && node scripts/fix-preload.js",
    "fetch-docs": "node scripts/fetch-documentation.mjs",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,scss,json,md}\"",
    "preview": "vite preview",
    "test": "jest",
    "prepublishOnly": "rimraf dist-lib && npm run build:lib:all",
    "postpublish": "rimraf dist-lib"
  },
  "dependencies": {
